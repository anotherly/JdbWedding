<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/C_All">


    <ScrollView
            android:layout_width="380dp"
            android:layout_height="match_parent"
            android:id="@+id/scrollA" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.0">

        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            ///////////////////////
            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:id="@+id/C_B1"
                    android:layout_marginEnd="10dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
                    app:layout_constraintHorizontal_bias="1.0"
                    android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="81dp"
                        android:layout_height="33dp"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/C_S1"
                        android:layout_marginTop="8dp" android:layout_marginStart="8dp">

                    <TextView
                            android:text="이성찾기"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:id="@+id/txtMsg1"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                </android.support.constraint.ConstraintLayout>
                <android.support.constraint.ConstraintLayout
                        android:layout_width="324dp"
                        android:layout_height="66dp"
                        android:id="@+id/C_M1"
                        android:layout_marginTop="10dp"
                        app:layout_constraintTop_toBottomOf="@+id/C_S1"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                >

                    <Button
                            android:text="연령대별"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn1"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="16dp"/>
                    <Button
                            android:text="지역별"
                            android:layout_width="wrap_content"
                            android:layout_height="48dp"
                            android:id="@+id/btn2"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintStart_toEndOf="@+id/btn1" android:layout_marginStart="8dp"
                            app:layout_constraintEnd_toStartOf="@+id/btn3" android:layout_marginEnd="8dp"
                    />
                    <Button
                            android:text="관심사별"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn3"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="16dp"/>
                </android.support.constraint.ConstraintLayout>
            </android.support.constraint.ConstraintLayout>
            /////////////////////
            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:id="@+id/C_B2"
                    android:layout_marginEnd="10dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
                    app:layout_constraintHorizontal_bias="1.0"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/C_B1">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="81dp"
                        android:layout_height="33dp"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/C_S2"
                        android:layout_marginTop="8dp" android:layout_marginStart="8dp">

                    <TextView
                            android:text="쪽지보기"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:id="@+id/txtMsg2"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="15dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="14dp" android:layout_marginTop="7dp"
                            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="6dp"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                </android.support.constraint.ConstraintLayout>
                <android.support.constraint.ConstraintLayout
                        android:layout_width="324dp"
                        android:layout_height="66dp"
                        android:id="@+id/C_M2"
                        android:layout_marginTop="10dp"
                        app:layout_constraintTop_toBottomOf="@+id/C_S2"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                >

                    <Button
                            android:text="공지사항"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn4"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="16dp"/>
                    <Button
                            android:text="보낸쪽지함"
                            android:layout_width="wrap_content"
                            android:layout_height="48dp"
                            android:id="@+id/btn5"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintStart_toEndOf="@+id/btn4" android:layout_marginStart="8dp"
                            app:layout_constraintEnd_toStartOf="@+id/btn6" android:layout_marginEnd="8dp"
                    />
                    <Button
                            android:text="받은쪽지함"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn6"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="16dp"/>
                </android.support.constraint.ConstraintLayout>
            </android.support.constraint.ConstraintLayout>
            //////////////////////////////
            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:id="@+id/C_B3"
                    android:layout_marginEnd="10dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/C_B2">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="81dp"
                        android:layout_height="33dp"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/C_S3"
                        android:layout_marginTop="12dp" android:layout_marginStart="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/C_M3" android:layout_marginEnd="8dp"
                        app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toTopOf="@+id/C_M3">

                    <TextView
                            android:text="내정보"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:id="@+id/txtMsg3"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintVertical_bias="0.666" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp"/>
                </android.support.constraint.ConstraintLayout>
                <android.support.constraint.ConstraintLayout
                        android:layout_width="324dp"
                        android:layout_height="66dp"
                        android:id="@+id/C_M3"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                        app:layout_constraintHorizontal_bias="0.4" android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toBottomOf="parent">

                    <Button
                            android:text="소개변경"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn7"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="16dp"/>
                    <Button
                            android:text="결제내역"
                            android:layout_width="wrap_content"
                            android:layout_height="48dp"
                            android:id="@+id/btn8"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintStart_toEndOf="@+id/btn7" android:layout_marginStart="8dp"
                            app:layout_constraintEnd_toStartOf="@+id/btn9" android:layout_marginEnd="8dp"
                    />
                    <Button
                            android:text="수정&탈퇴"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn9"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="16dp"/>
                </android.support.constraint.ConstraintLayout>
            </android.support.constraint.ConstraintLayout>
            ////////////////////////////////////
            <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:id="@+id/C_B4"
                    android:layout_marginEnd="10dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="10dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/C_B3">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="81dp"
                        android:layout_height="33dp"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/C_S7"
                        android:layout_marginTop="8dp" android:layout_marginStart="8dp">

                    <TextView
                            android:text="자유게시판"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:id="@+id/txtMsg4"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                </android.support.constraint.ConstraintLayout>
                <android.support.constraint.ConstraintLayout
                        android:layout_width="324dp"
                        android:layout_height="66dp"
                        android:id="@+id/C_M4"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                        android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/C_S7">

                    <Button
                            android:text="이벤트"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn10"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="16dp"/>
                    <Button
                            android:text="사진게시판"
                            android:layout_width="wrap_content"
                            android:layout_height="48dp"
                            android:id="@+id/btn11"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintStart_toEndOf="@+id/btn10" android:layout_marginStart="8dp"
                            app:layout_constraintEnd_toStartOf="@+id/btn12" android:layout_marginEnd="8dp"
                    />
                    <Button
                            android:text="질문및답변"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/btn12"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="9dp" app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="16dp"/>
                </android.support.constraint.ConstraintLayout>
            </android.support.constraint.ConstraintLayout>
            ///////////////////////////////////
        </android.support.constraint.ConstraintLayout>


    </ScrollView>
</android.support.constraint.ConstraintLayout>